<resources xmlns:tools="http://schemas.android.com/tools">

    <!--dialog 没有横条背景-->
    <style name="Theme.AppStartLoadTranslucent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/tab_bottom_font1</item> <!--小强在线 标准色系-->
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- 设置页 command -->
    <style name="setting_command_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/item_meeting</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <!--通讯录页面-->
    <!--列表头-->
    <style name="ContactItemHeader">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/contact_item_header_height</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/whiteE</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingStart" tools:ignore="NewApi">@dimen/activity_horizontal_margin
        </item>
        <item name="android:textColor">@color/black4</item>
        <item name="android:textSize">@dimen/contact_item_header_font</item>
    </style>

    <style name="Theme_dialogstyle" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@animator/push_up_in</item>
        <item name="@android:windowExitAnimation">@animator/push_up_out</item>
    </style>

    <style name="Theme_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!--更新-->
